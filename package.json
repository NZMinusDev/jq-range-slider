{
  "name": "jq-range-slider",
  "version": "1.0.0",
  "private": true,
  "author": "Nikita Zaitsev <developer.n.zaytsev@gmail.com>",
  "license": "MIT",
  "description": "It's jq-range-slider plugin using MVP on typescript",
  "keywords": [
    "BEM",
    "html",
    "pug",
    "css",
    "scss",
    "js",
    "ts",
    "javascript",
    "typescript",
    "webpack",
    "MVP",
    "JQuery",
    "slider"
  ],
  "browserslist": [
    "> 0.2%",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "start": "npm-run-all --parallel watch:client watch:server",
    "watch:client": "rm -r -f app/public && cross-env NODE_ENV=development MEASURE=false webpack-dev-server --config configs/webpack.config.js --mode development",
    "watch:server": "cross-env NODE_ENV=development nodemon app/src/server/index.mjs",
    "dev": "cross-env NODE_ENV=development MEASURE=false webpack --config configs/webpack.config.js --mode development",
    "build": "cross-env NODE_ENV=production MEASURE=true webpack --config configs/webpack.config.js --mode production",
    "build:plugin": "cross-env NODE_ENV=production MEASURE=true PLUGIN_ONLY=true webpack --config configs/webpack.config.js --mode production && npm run types",
    "types": "ttsc app/src/plugin/presenter/RangeSliderPresenter.ts --declaration --emitDeclarationOnly --declarationDir app/dist/types",
    "UML": "tplant --input app/src/plugin/presenter/RangeSliderPresenter.ts --output app/src/plugin/UML/UML.puml",
    "test": "jest --config=configs/jest.config.js --watchAll",
    "analyze": "mkdirp app/log && cross-env NODE_ENV=production MEASURE=false PLUGIN_ONLY=true webpack --config configs/webpack.config.js --mode production --json > app/log/stats.json && webpack-bundle-analyzer app/log/stats.json",
    "lint": "stylelint \"./app/src/**/*.scss\" && eslint ./app/src/ --ext .js,.ts",
    "lint:fix": "prettier ./app/src/ --ignore-unknown --check --write && stylelint \"./app/src/**/*.scss\" --fix && eslint ./app/src/ --ext .js,.ts --fix",
    "deploy": "git subtree push --prefix app/public origin gh-pages"
  },
  "stylelint": {
    "extends": "./configs/stylelint.config.js"
  },
  "eslintConfig": {
    "extends": "./configs/.eslintrc.js"
  },
  "prettier": "./configs/prettier.config.js",
  "devDependencies": {
    "@prettier/plugin-pug": "^1.15.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.20",
    "@types/jquery": "^3.5.2",
    "@types/lodash-es": "^4.14.168",
    "@types/stylelint": "^13.13.2",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-jest": "^26.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-fsd": "^1.0.1",
    "eslint-plugin-lit": "^1.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "open-browser-webpack-plugin": "github:rodrigopandini/open-browser-webpack-plugin",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.21.0",
    "stylelint-webpack-plugin": "^2.2.2",
    "tplant": "^2.3.6",
    "ts-jest": "^26.5.0",
    "ttypescript": "^1.5.12",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@open-wc/lit-helpers": "^0.3.12",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "bemdecl-to-fs-loader": "0.0.4",
    "circular-dependency-plugin": "^5.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "doiuse": "^4.2.0",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.4.1",
    "html2bemdecl-loader": "^1.0.5",
    "inspectpack": "^4.5.2",
    "jquery": "^3.5.1",
    "lit-element": "^2.5.0",
    "lit-html": "^1.3.0",
    "lodash-es": "^4.17.15",
    "mini-css-extract-plugin": "^0.11.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^4.0.1",
    "postcss-preset-env": "^6.7.0",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "speed-measure-webpack-plugin": "^1.3.3",
    "terser-webpack-plugin": "^4.2.0",
    "typescript": "^4.3.5",
    "unused-files-webpack-plugin": "^3.4.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
